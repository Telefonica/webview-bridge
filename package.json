{
    "name": "@novum/webview-bridge",
    "version": "1.0.0",
    "description": "",
    "main": "./dist/webview-bridge-cjs.js",
    "module": "./dist/webview-bridge.mjs",
    "types": "./dist/index.d.ts",
    "scripts": {
        "build": "npm run clean && rollup -c && uglifyjs --compress --mangle -o dist/webview-bridge-iife.min.js dist/webview-bridge-iife.js",
        "test": "nyc --reporter=text-summary --reporter=lcov ava --verbose --serial test/test.js",
        "clean": "del dist && del coverage && del .nyc_output",
        "report": "nyc report --reporter=html && opn coverage/index.html",
        "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
        "lint": "tslint *.ts",
        "prepublishOnly": "npm run clean && npm run lint && npm run build && npm test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tef-novum/webview-bridge.git"
    },
    "author": "appscore@tuenti.com",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/tef-novum/webview-bridge/issues"
    },
    "homepage": "https://github.com/tef-novum/webview-bridge#readme",
    "dependencies": {},
    "devDependencies": {
        "coveralls": "^3.0.2",
        "del-cli": "^1.1.0",
        "husky": "^1.2.0",
        "lint-staged": "^8.0.5",
        "nyc": "^13.1.0",
        "opn-cli": "^4.0.0",
        "prettier": "^1.15.2",
        "rollup": "^0.67.3",
        "rollup-plugin-typescript2": "^0.18.0",
        "tslint": "^5.11.0",
        "typescript": "^3.1.6",
        "uglify-es": "^3.3.9"
    }
}
